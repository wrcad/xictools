#! /bin/sh

# Echo files found in fastcap distribution.

accs="\
fcpp \
lstpack \
lstunpack"

bin="\
fastcap \
cubegen \
capgen \
busgen \
pipedgen \
pyragen"

docs="\
fastcap_paper.pdf \
FastCapsuppl.pdf \
mtt.ps \
tcad.ps \
ug.ps"

examples="\
1x1bus.lst \
1x1coarse.neu \
1x1fine.neu \
1x1nonuni.neu \
big_sphere1.neu \
coated_sph.lst \
cond_air_1x1.qui \
cond_dielec_1x1.qui \
cubeeg.fig \
dielec_face.neu \
dielec_sides_2x2.neu \
dielec_sides.neu \
dielec_topbot.neu \
pin \
pipeeg.fig \
ramcell_bitl.neu \
ramcell_botd.neu \
ramcell_cond.neu \
ramcell_gnd.neu \
ramcell.lst \
ramcell.out \
ramcell_test.neu \
ramcell_topd.neu \
README \
sphere1.neu \
sphere2.neu \
sphere3.neu \
via.neu"

examples_scripts="\
connector2.sh \
connector3.sh \
connector4.sh \
ramgen.sh \
testgen.sh \
testrun.sh"

results="\
1x1bus.out \
1x1coarse.out \
1x1fine.out \
1x1nonuni.out \
big_sphere1.out \
coated_sph.out \
dielec_face.out \
dielec_sides_2x2.out \
dielec_sides.out \
dielec_topbot.out \
ramcell_bitl.out \
ramcell_botd.out \
ramcell_cond.out \
ramcell_gnd.out \
ramcell.out \
ramcell_test.out \
ramcell_topd.out \
sphere1.out \
sphere2.out \
sphere3.out \
via.out"

case $1 in
accs)
    echo $accs;;
bin)
    echo $bin;;
doc)
    echo $docs;;
examples)
    echo $examples;;
examples_scripts)
    echo $examples_scripts;;
results)
    echo $results;;
esac

