mextram \
psp102 \
psp103"

case $1 in
admst)
    echo $admst;;
vlmods)
    echo $vlmods;;
bin)
    echo $bin;;
devincl)
    echo $devincl;;
dkexdirs)
    echo $dkexdirs;;
docs)
    echo $docs;;
examples)
    echo $examples;;
JJexamples)
    echo $JJexamples;;
JJexamples_old)
    echo $JJexamples_old;;
help)
    echo $help;;
help_screenshots)
    echo $help_screenshots;;
icons)
    echo $icons;;
scripts)
    echo $scripts;;
startup)
    echo $startup;;
klu)
    case x$2 in
    xDarwin*)
        KLU_PLUGIN=klu_wr.dylib;;
    xDebian | xLinux* | xFreeBSD*)
        KLU_PLUGIN=klu_wr.so;;
    xWin*)
        KLU_PLUGIN=klu_wr.dll;;
    esac
    echo $KLU_PLUGIN;;
soext)
    case x$2 in
    xDarwin*)
        echo dylib;;
    xDebian | xLinux* | xFreeBSD*)
        echo so;;
    xWin*)
        echo dll;;
    esac;;
modname)
    case x$2 in
    xbsim6)
        echo bsim6;;
    xbsim6.1)
        echo bsim6;;
    xbsimcmg)
        echo bsimcmg;;
    xbsoi44)
        echo bsoi44;;
    xbsoi45)
        echo bsoi45;;
    xcap)
        echo cap;;
    xekv)
        echo ekv26;;
    xhicum0)
        echo hicum0;;
    xhicum2)
        echo hicum2;;
    xind)
        echo ind;;
    xjj)
        echo jj;;
    xmextram)
        echo bjt504;;
    xpsp102)
        echo psp102;;
    xpsp103)
        echo psp103;;
    esac;;
esac

